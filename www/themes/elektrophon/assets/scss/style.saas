.navbar-brand {
    font: 36px Fira+Sans;
}
.hero-background {
    position: absolute;
    object-fit: cover;
    object-position: center center;
    width: 100%;
    height: 100%;
}
.hero-background.is-transparent {
    opacity: 0.8;
}
.hero-darken {
    background-color: rgba($hero-darken, 0.5);
}
.navbar-brand a {
font-family: Montserrat;
padding: 5px 16px;
}